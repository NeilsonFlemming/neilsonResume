---
interface Props {
  class?: string;
}
const { class: className = 'w-5 h-5' } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
  aria-hidden="true"
>
  <polyline points="6 9 12 15 18 9"></polyline>
</svg>
